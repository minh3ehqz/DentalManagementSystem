@model List<Patient> // Model




<table>
    <tr>
        <td>
            Id
        </td>
        <td>
            Name
        </td>
    </tr>

    @{
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
            </tr>
        }
    }
</table>